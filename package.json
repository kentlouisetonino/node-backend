{
  "name": "node-backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "start": "tsc --build && node ./dist/server.js",
    "dev": "nodemon --config nodemon.json ./src/server.ts",
    "format": "prettier --write \"./**/*.{html,css,ts,js,json,prettierrc,yml}\"",
    "test:all": "jest --watchAll --verbose --coverage --runInBand --no-cache"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/nodemon": "^1.19.6",
    "@types/prettier": "^3.0.0",
    "@types/superagent": "^8.1.4",
    "@types/supertest": "^6.0.1",
    "aws-sdk": "^2.1581.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.7.0",
    "babel-preset-env": "^1.7.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jest": "^29.7.0",
    "mongodb": "^6.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "superagent": "^8.1.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/coverage/"
    ]
  }
}
